{
    "name": "{{cookiecutter.project_name}}",
    "license": "{{cookiecutter.license}}",
    "scripts": {
        "up": "cd wp_{{cookiecutter.project_slug}} && docker-compose up -d",
        "down": "cd wp_{{cookiecutter.project_slug}} && docker-compose down",
        "logs": "cd wp_{{cookiecutter.project_slug}} && docker-compose logs",
        "volumerm": "cd wp_{{cookiecutter.project_slug}} && docker volume rm {{cookiecutter.wp_volume_name}} {{cookiecutter.db_volume_name}}",
        "build": "cd wp_{{cookiecutter.project_slug}} && docker-compose build"
    }
}